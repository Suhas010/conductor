<h1>Edit Files</h3>
<%= form_for(:code, url: update_code_path(@code.path), method: :patch) do |f| %>
  <p>
    <div id="editor"></div>
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.submit "Save File" %>
  </p>
<% end %>
<%= content_for :scripts do %>
  <%= javascript_include_tag "conductor/editor" %>
  <%= javascript_include_tag "conductor/codes" %>
<% end %>
